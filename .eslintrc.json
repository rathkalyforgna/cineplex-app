{
  "extends": ["plugin:@tanstack/eslint-plugin-query/recommended","next/core-web-vitals"],
  "plugins": ["@tanstack/query"],
  "rules": {
    "eqeqeq": "error",
    "no-console": "warn",
    "@tanstack/query/exhaustive-deps": "error",
    "@tanstack/query/prefer-query-object-syntax": "error"
